{
    "name": "Zombie Simulation Dev Container",
    "dockerFile": "Dockerfile",
    "settings": {
        "terminal.integrated.defaultProfile.linux": "bash"
    },
    "extensions": [
        "ms-python.python"
    ],
    "forwardPorts": [8000],
    "postCreateCommand": "pip install -r /workspace/requirements.txt",
    "remoteUser": "vscode"
}
